class TelnetRequest extends WAClass implements Request
{
  public $protocol = null;
  public $client = null;
  public $shell = null;

  private $data = '';

  public function __construct()
  {
    parent::__construct();
    if (self::$debug || $this->localdebug)
      $this->doDebug("TelnetRequest->__construct()", WADebug::SYSTEM);
  }

  public function clear()
  {
// Those 2 basic object are supposed to NOT change if the request is reused...
//     $this->protocol = $this->client = null;
    $this->data = '';
  }

  public function addData($data)
  {
    $this->data .= $data;
  }

  public function getData()
  {
    return $this->data;
  }
}

