class TelnetResponse extends WAClass implements Response
{
  public $content = false;

  private $data = '';

  public function __construct()
  {
    parent::__construct();
    if (self::$debug || $this->localdebug)
      $this->doDebug("TelnetResponse->__construct()", WADebug::SYSTEM);
  }

  public function clear()
  {
    $this->content = false;
    $this->data = '';
  }

  public function addData($data)
  {
    $this->data .= $data;
  }

  public function getData()
  {
    return $this->data;
  }

}

?>